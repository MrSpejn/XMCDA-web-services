<?xml version='1.0' encoding='utf-8'?>
<program_description>
  <program provider="PUT" name="Electre_I_Kernel" version="1.0.0" displayName="Electre_I_Kernel" />
  <documentation>
    <description></description>
    <contact><![CDATA[Tomasz Josiak <tomasz.josiak@gmail.com>]]></contact>
  </documentation>
  <parameters>

    <input id="input1" name="alternatives" displayName="alternatives" isoptional="0">
      <documentation>
        <description>Selected alternatives.</description>
      </documentation>
      <xmcda tag="alternatives" />
    </input>

    <input id="input2" name="outranking_matrix" displayName="Outranking Matrix" isoptional="0">
      <documentation>
        <description>Alternatives values where 1 means selection and any other value means rejection</description>
      </documentation>
      <xmcda tag="alternativesMatrix" />
    </input>

    <output id="output1" name="alternatives" displayName="Alternatives">
      <documentation>
        <description>Aggregated alternatives of acyclic graph</description>
      </documentation>
      <xmcda tag="alternatives" />
    </output>


    <output id="output2" name="kernel" displayName="Kernel">
      <documentation>
        <description>Alternatives values specifying which alternative belong to the kernel</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </output>

    <output id="output3" name="outranking_matrix" displayName="Outranking Matrix">
      <documentation>
        <description>Outranking matrix of acyclic graph generated from the input</description>
      </documentation>
      <xmcda tag="alternativesMatrix" />
    </output>

    <output id="output4" name="messages" displayName="messages">
      <documentation>
        <description>Messages or errors generated by this module.</description>
      </documentation>
      <xmcda tag="methodMessages" />
    </output>

  </parameters>
</program_description>
